public void salto (vertex salto){
  stack s = new stack();
  v = lugar vacio saltado;
  marcar negra saltada;
  s.push(v);
  while (!s.isEmpty()){
    vertex v=s.top();
    vertex w = pos vacia a saltar(u);
    if (w!=null){
      saltar;
      marcar negra saltada;
      s.push(w);
    }
    else{
      if (negras==total negras marcadas){
        "se puede ganar"
      }
      else{
        while(!s.isEmpty(){
          vertex aux = s.top();
          aux desmarcar;
          s.pop();
        }
      }
    }
  }
}
