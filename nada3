public class ficha extends nodo implements Interficha {

  private  Int color;//0->B; 1->N;
  private  Pair pos;

  public ficha (Int col,Pair poss){
    color=col;
    pos=poss;
  }
  public void  setPos( Pair poss){
    pos=poss;
  }

  public Int  getColor(){
    return color;
  }
  public Piair getPos (){
    return pos;
  }
}
